{
  "name": "@coaching/ui",
  "version": "0.1.0",
  "private": true,
  "description": "Shared UI components for the coaching platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
