{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "RAILPACK",
    "installCommand": "corepack enable && pnpm install",
    "buildCommand": "pnpm build"
  },
  "deploy": {
    "startCommand": "pnpm start",
    "healthcheckPath": "/",
    "restartPolicyType": "ON_FAILURE"
  }
}
